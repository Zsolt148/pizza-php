{
  "devDependencies": {
    "tailwindcss": "^3.2.0"
  },
  "scripts": {
    "dev": "npm run development",
    "development": "npx tailwindcss -i ./resources/css/app.css -o ./public/css/app.css",
    "watch": "npx tailwindcss -i ./resources/css/app.css -o ./public/css/app.css --watch",
    "prod": "npm run production",
    "production": "npx tailwindcss -i ./resources/css/app.css -o ./public/css/app.css --minify"
  }
}
